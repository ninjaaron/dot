#!/usr/bin/env python3
import subprocess as sp
import sys, re
torrents = sp.check_output(['transmission-remote', '-l']).decode().split('\n')
for tor in (t for t in torrents if re.search(sys.argv[1], t, re.I)):
    tor = re.split(' {2,}', tor.strip())
    print('{0:3}  {1:>4}  {3:8}  {8}  {2}  {5}'.format(*tor))
